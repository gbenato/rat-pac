{
name: "DAQ",
valid_begin: [0, 0],
valid_end: [0, 0],
channel_efficiency: 1.0,

//Dark hits noise
noise_rate: 0.0,

//Pulses
pulse_type: 1, //0=square pulses, 1=realistic pulses
               //must change pulse_width depending on selection
pulse_width: 0.5, //0.15, 0.22 PMT pulse width in log(ns) for real pulses
pulse_offset: 0.0, //PMT pulse offset in pA
pulse_min: -0.00001, //minimum pulse height to consider in mV
pulse_mean: 5.5, //1.7 PMT pulse mean in ns
pulse_step_time: 0.001, //time step in ns

//Electronic noise
noise_amplitude: 0.0, //amplitude of high frequency noise in mV

//Digitizer
nbits: 14, //number of bits
step_time: 2.0, //time resolution in ns
volt_high: 1000.0, //upper voltage limit in mV
volt_low: -1000.0, //lower voltage limit in mV
offset: -500.0, //voltage offset in mV
resistance: 50.0, //resistance of the output circuit in Ohms
sampling_time: 1000.0, //full gate for grouping hits in a single integration in ns
trigger_threshold: -5.0, //PMT threshold to issue a trigger mV
gate_delay: 400.0, //time before discriminator fires that sampling gate opens (in ns)

//Pulse analysis
ped_start: 0.0, //start time of pedestal window in ns
ped_end: 300.0, //end time of pedestal window in ns
max_spread: 3.0, //
int_start: 300.0, //start time of integration window in ns
int_end: 1000.0 //start time of integration window in ns


}
